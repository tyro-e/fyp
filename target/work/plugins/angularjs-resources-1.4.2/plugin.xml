<plugin name='angularjs-resources' version='1.4.2' grailsVersion='2.2 &gt; *'>
  <author>Sandro Martini, originally developed by Vladimír Oraný</author>
  <authorEmail>sandro.martini@gmail.com</authorEmail>
  <title>AngularJS Resources Plugin</title>
  <description>Angular JS resources plugin</description>
  <documentation>http://smartiniongithub.github.com/grails-angularjs-resources/</documentation>
  <type>AngularjsResourcesGrailsPlugin</type>
  <resources>
    <resource>AngularjsResourcesConfig</resource>
    <resource>AngularjsResourcesPluginResources</resource>
    <resource>AngularjsResourcesUrlMappings</resource>
    <resource>grails_angularjs.AngularJsAjaxFilters</resource>
    <resource>angularjs.Nge2eController</resource>
    <resource>grails_angularjs.AngularJsService</resource>
  </resources>
  <repositories>
    <repository name='grailsCentral' url='http://grails.org/plugins' />
    <repository name='mavenCentral' url='http://repo1.maven.org/maven2/' />
    <repository name='https://repo.grails.org/grails/plugins' url='https://repo.grails.org/grails/plugins/' />
  </repositories>
  <dependencies />
  <plugins />
  <runtimePluginRequirements />
  <behavior />
</plugin>